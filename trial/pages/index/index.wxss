/**index.wxss**/
page {height: 100%;background: #f5f4f9}
.left {float: left;}
.trial {}
.lxkw-warning-bar-yellow-1 {    background-color: #fbe7c6;color: #f18629;height: 37px;line-height: 37px;text-align: center; width: 100%;font-size: 12px;}
.submit-c-container {width: 100%;background-color: #fff;line-height: 42px;height: 42px;border-top: 0.5px solid #e5e5e5;border-bottom: 0.5px solid #e5e5e5;margin: 15px 0 15px 0;}
.student-name {width: 100%;background-color: #fff;line-height: 42px;height: 42px;margin: 0 0 15px 0;position: relative;}
.student-name label {color: #000;width: 90px;font-size: 17px;line-height: 40px;}
.student-name #studentName {border: none;line-height: 40px;font-size: 17px;color: #000;height: 40px;width:calc(100% - 110px);}
.information {margin: 15px 0;padding-left: 15px;background-color: #fff;border-top: 0.5px solid #e5e5e5;border-bottom: 0.5px solid #e5e5e5;}
.try-information .try-item {border-bottom: 0.5px solid #e5e5e5;margin-bottom: 0px;}
.student-name .arror-right {float: right;width: 13px;height: 17px;background-size: 12px 17px;position: absolute;right: 10px;top:2px;}
.student-name .arror-right image {width: 100%;width: 13px;height: 17px;}
.submit {width: calc(100% - 20px);margin-left: 10px;margin-top: 30px; position: relative;margin-bottom:20px;}
.picker {font-size: 17px;color: #000;}
.picker span {margin-left: 25px;}
.g-link {position: absolute;width: 100%;height: 100%;z-index: 2;top:0px}