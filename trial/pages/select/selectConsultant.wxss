/* pages/select/selectConsultant.wxss */
/*弹窗样式*/
page {background: #f5f4f9;}
.select-body {padding-bottom:107px;}
.lxkw-touming-background{background: rgba(0, 0, 0, 0.7) none repeat scroll 0% 0%;position: fixed;z-index: 3;width: 100%;height: 100%;top: 0px;left: 0px;}
.market-info {}
.new-trial-marker-text-body {position: absolute;bottom: 95px;width: 100%;}
.new-trial-marker-text-body .new-trial-marker-text {padding: 5px 5px 15px;margin: 10px auto;width: 176px;background: transparent url("../images/alert-bg.png")no-repeat scroll 0% 0% / 176px 70px;background-size: 100% 100%;font-size: 14px;line-height: 20px;background:#fff}
.marker-logo-body {position: fixed;z-index: 2;margin: auto;left: 0;right: 0;bottom: 10px;width: 66px;}
.marker-logo-body .marker-logo {width: 66px;z-index: 5;color: rgb(255, 255, 255);border-radius: 3px;border: 1px solid #3fb88c;background: rgba(63, 184, 140, 0.8) none repeat scroll 0% 0%;box-shadow: 2px 2px 2px 1px rgba(0, 0, 0, 0.2);margin: 0px auto;height:86px;font-size: 12px;text-align: center;}
.marker-logo-body .lxkw-secondary-button-1 {margin: 0px auto;position: absolute;bottom: 28px;left: 127%;width: 200px;background-size: 85px 34px;height: 34px;background-position: center;}
.marker-logo-body .lxkw-secondary-button-1 image {width: 100px;height: 34px;}
.marker-logo image {width: 66px; height: 64px;} 
.marker-logo .market-name {}
.select-alert {position: fixed;bottom: 20%;width: calc(100% - 50px);margin-left: 25px;text-align: center;}
.select-alert .select-alert-top {font-size: 15px;color: #fff;}
.select-alert .select-alert-body {border-radius: 5px;text-align: left;background: #fff;padding: 13px;margin-top: 5px;}
.select-alert .select-alert-body .radio-group label {line-height: 20px;margin-top: 10px;font-size: 15px;color: #6b6d6e}
.select-alert .select-alert-body .radio-group .radio-group-text {font-size: 12px;padding-left:30px;margin-bottom:10px;}
.select-alert-button {margin: 10px auto;background-position: center;}
.select-alert-button image {width: 100px;height: 34px;}
.no-select-tip {margin-top: 10px;color: #f96268;font-size: 14px;}

/*试用详情*/
.select-student-intro {margin-bottom: 8px;background-color: #dcecf8;border: none;}
.lxkw-public-list-item {padding: 10px;border-bottom: none;}
.lxkw-public-list-item-details-img {height: 44px;width: 44px;}
.lxkw-public-list-item-details-img image {height: 44px;width: 44px;border-radius: 3px;}
.lxkw-public-list-item-details-content {margin-left: 50px;color: #373737;padding-top: 2px;position: relative;}
.lxkw-public-list-item-details-content .student-trial-name {font-size: 15px;line-height: 15px;}
.list-details-small-title {color: #6B6D6E;font-size: 12px;margin-top: 12px;line-height: 14px;}
.list-details-small-title .remain-teacher {color: #ffb359;font-size: 15px;padding: 0 3px;}
.submit-buttun {position: absolute;font-size: 12px;background: #03aaee;color: #fff;right: 0px;top: 7px;height: 30px;line-height: 30px;padding: 0px 5px;border-radius: 5px;}
.lxkw-controls-topnav-select {margin: 0 auto;height: 40px;width: 100% !important;background: rgb(255, 255, 255) none repeat scroll 0% 0%;}
.lxkw-controls-topnav-select .lxkw-controls-topnav-select-span {width: 50%;height: 100%;line-height: 40px;margin-top: 0px;border: medium none;border-radius: 0px;float: left;font-size: 12px;text-align: center;display: inline-block;}
.lxkw-controls-topnav-select .select {background: #03aaee;color: #fff;}
.lxkw-controls-topnav-select .lxkw-controls-topnav-select-span .select-arror {background: url(http://static.liuxuekw.com/theme/default/images/trial/arror-right.png);display: inline-block;width: 7px;height: 8px;background-size: 7px 8px;}
.lxkw-controls-tab-rank {background: rgb(255, 255, 255) none repeat scroll 0% 0%; margin: 5px 0px 0;position: relative;}
.lxkw-controls-tab-rank .lxkw-controls-tab-rank-tab {width: 50%;line-height: 44px;font-size: 15px;display:inline-block;text-align:center;}
.lxkw-controls-tab-rank .select {color:#03aaee}

/*x下啦框*/
.lxkw-controls-tab-rank-down-list {border-top: 10px solid #f5f4f9;}
.lxkw-controls-tab-rank-down-list .list-item {height: 50px; background: rgb(255, 255, 255) none repeat scroll 0% 0%; line-height: 50px; font-size: 15px; padding: 0px 20px; border-bottom: 1px solid rgb(227, 227, 227);position: relative;}
.lxkw-controls-tab-rank-down-list .list-item p {display: block;font-size: 15px;color: #6b6d6e;line-height:25px;}
.lxkw-controls-tab-rank-down-list .list-item icon {position: absolute;right: 10px;top: 10px;display: none}
.lxkw-controls-tab-rank-down-list .list-item.select icon {display: block}
.lxkw-controls-tab-rank-down-list .list-item:last-child {border: none;}
.lxkw-controls-tab-rank-down-list .list-item span {float: right; margin-top: 17px; font-size: 14px; color: #fff;}
.lxkw-controls-tab-rank-down-list .list-item.select {color: #03aaee;}
.lxkw-controls-tab-rank-down-list .list-item.select p {color: #03aaee;}
.lxkw-touming-background-part {z-index: 2;width: 100%;height: calc(100% + 50px);left: 0px;background: rgba(0, 0, 0, 0.4) none repeat scroll 0% 0%;position: absolute}

/*顾问列表*/
.consultant {}
.consultant .consultant-item {margin-top: 10px}
.consultant .consultant-item .match-details{padding: 10px 0;position:relative;
}
.consultant-item .consultant-item-top {position: relative;background: #fff;padding: 10px;}
.consultant-item-top .consultant-name {font-size: 15px;color: #373737}
.consultant-item-top .consultant-name span {font-size: 12px;color: #6b6d6e}
.consultant-item-top .list-item-details {margin-top: 10px;font-size: 13px;color: #6b6d6e;}
.list-item-details .consultant-location {color:#6b6d6e;background-image: url(http://static.liuxuekw.com/theme/default/images/lxkw-common-img/location.png);background-repeat: no-repeat;background-size: 10px 12px;background-position: left;padding-left: 14px;margin-right: 10px;}
.list-item-details .consultant-year {background-image: url(http://static.liuxuekw.com/theme/default/images/new-index/consultant-year-logo.png);background-repeat: no-repeat;background-size: 12px 12px;background-position: left;padding-left: 14px;color: #6b6d6e;}
.list-item-details .consultant-demosit {color: #6b6d6e;margin-right: 10px;background: url(http://static.liuxuekw.com/theme/default/images/new-index/demosit-amount-logo.png)no-repeat;background-size: 11px 13px;padding-left: 14px;}
.list-item-details .consultant-three-money {background: url(http://static.liuxuekw.com/theme/default/images/trialDetail/new-select-three-money-logo.png)no-repeat;
background-size: 13px 13px;
padding-left: 14px;
color: #6b6d6e;}
.consultant-item-top .arror-right {float: right;width: 13px;height: 17px;background-size: 12px 17px;position: absolute;right: 10px;top:20px;}
.match-details .match-title {height: 35px;color: #ffb359;border-bottom: 2px solid #ffb359;padding: 0 10px;font-size: 12px;line-height: 35px;}
.match-details .match-title span {font-size: 15px;}
.match-alignment {padding: 10px 10px 20px;position: relative;}
.match-alignment .percentage-item {margin-top: 10px;height: 17px;line-height: 17px;}
.match-alignment .percentage-item .star-title {font-size: 11px;color: #373737;vertical-align: middle;margin-right: 15px;}
.lxkw-controls-star .lxkw-controls-star-content {margin: 0 auto;position: relative;width: 100px;height: 15px;}
.lxkw-controls-star-content .star-top {display: block;width: 100%;height: 100%;background: url(http://static.liuxuekw.com/theme/default/images/lxkw-common-img/star-show-1.png)no-repeat;background-size: 100px 15px;}
.lxkw-controls-star-content .star-width {position: absolute;overflow: hidden;width: 100%;height: 100%;top: 0;}
.lxkw-controls-star-content .star-width .star-bottom {display: block;width: 100%;height: 100%;background: url(http://static.liuxuekw.com/theme/default/images/lxkw-common-img/star-show.png)no-repeat;background-size: 100px 15px;}
.match-alignment .match-arrow {position: absolute;right: 10px;top: 45%;color: #aaa;font-size: 12px;}

.consultant-response-title {padding: 10px;font-size: 15px;padding-bottom: 5px;color: #373737;}
.consultant-response-content {padding: 0px 10px;font-size: 12px;color: #6b6d6e;}
.consultant-response-content view {font-size: 12px;color: #6b6d6e;margin-top: 10px;line-height: 19px;}
.consultant-response-content view span {font-size: 15px;color: #ffb359;}
.new-select-grade {padding: 10px;color: #373737;font-size: 12px;}
.new-select-grade .content {line-height: 30px;}
.new-select-grade .content span {color: #ffb359;font-size: 15px}

.new-select-case {padding: 5px 10px;font-size: 12px;}
.new-select-case span {color: #373737}

.consultant-item-bottom {background: #fff;height: 50px;}
.consultant-item-bottom .select-button {float: right;font-size: 13px;/*height: 30px*/border: 1px solid #03aaee;padding: 0 5px;color: #03aaee;line-height: 25px;margin: 9px;width: 80px;text-align: center;border-radius: 5px;}
.fixed-bottom-tips {height: 36px;line-height: 36px;text-align: center;font-size: 12px;color: rgb(255, 255, 255);background: rgb(255, 179, 89) none repeat scroll 0% 0%;}
.consultant-item-bottom .select-button-select {float: right;font-size: 13px;/*height: 30px*/border: 1px solid #aaa;padding: 0 5px;color: #aaa;line-height: 25px;margin: 9px;width: 80px;text-align: center;border-radius: 5px;}
.fixed-bottom-tips {height: 36px;line-height: 36px;text-align: center;font-size: 12px;color: rgb(255, 255, 255);background: rgb(255, 179, 89) none repeat scroll 0% 0%;}
.marker-logo-body {position: fixed;z-index: 2;margin: auto;left: 0;right: 0;bottom: 10px;width: 66px;}
.marker-logo-body .marker-logo{width: 66px;z-index: 5;color: rgb(255, 255, 255);border-radius: 3px;border: 1px solid #3fb88c;background: rgba(63, 184, 140, 0.8) none repeat scroll 0% 0%;box-shadow: 2px 2px 2px 1px rgba(0, 0, 0, 0.2);margin: 0px auto;}
.marker-logo-body .marker-logo image{border-radius: 3px 3px 0 0;height: 64px;width: 66px;}
.marker-logo-body .marker-logo view {font-size: 12px;height: 20px;line-height: 20px;text-align: center;width: 100%;}
.contact {position: absolute;width: 68px;height: 88px;top: 0px;background: rgba(0, 0, 0, 0);border: none;z-index: 5;}
